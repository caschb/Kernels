NUMITERS=10 
SEPLINE="===============================================================" 

STARPU/PIC/pic                  $NUMITERS 1000 1000000 1 2 GEOMETRIC 0.99;      echo $SEPLINE
STARPU/PIC/pic                  $NUMITERS 1000 1000000 0 1 SINUSOIDAL;          echo $SEPLINE
STARPU/PIC/pic                  $NUMITERS 1000 1000000 1 0 LINEAR 1.0 3.0;      echo $SEPLINE
STARPU/PIC/pic                  $NUMITERS 1000 1000000 1 0 PATCH 0 200 100 200; echo $SEPLINE